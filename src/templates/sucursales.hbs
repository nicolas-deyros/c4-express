<!DOCTYPE html>
<html lang="en">

<head>
  {{> partials/gtm.head}}
  <title>{{ htmlWebpackPlugin.options.title }}</title>
</head>

<body class="bg-orange-100">
  {{> partials/gtm.body}}
  <div class="md:flex flex-col md:flex-row md:min-h-screen w-full">

    <!-- navbar -->
    {{> partials/nav}}
    <!-- /navbar -->

    <!-- main -->
    <main
      class="mx-auto max-w-full px-4 md:max-w-screen-lg md:px-0 flex-1 overflow-y-auto md:flex flex-col items-center justify-center">
      <iframe src="https://www.google.com/maps/d/embed?mid=1QzPOGsWJQitrhMhjjpWo0CLMij9fum9e&hl=es-419" width="100%"
        height="500px" frameborder="0" class="mt-2"></iframe>

      <div id="wrapper" class="container mx-auto"></div>

    </main>
    <!-- /main -->


  </div>

  <!-- footer -->
  {{> partials/footer}}
  <!-- /footer -->
</body>

</html>